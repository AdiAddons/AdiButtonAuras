{
  "problemMatcher": [
    {
      "owner": "luacheck",
      "pattern": [
        {
          "regexp": "^(.+)\\((\\d+),(\\d+)\\) : (warning|error) ([EW]\\d+): (.+)$",
          "file": 1,
          "line": 2,
          "column": 3,
          "severity": 4,
          "code": 5,
          "message": 6
        }
      ]
    }
  ]
}
